<h1>Cats upon cats!</h1>

<ul>
  <% @cats.each do |cat| %>
    <li><a href="<%= cat_url(cat) %>"><%= cat.name %></a></li>
  <% end %>
</ul>

<% if current_user.nil? %>
  <a href="<%= new_session_url %>">Log in</a>
  <a href="<%= new_user_url %>">Register</a>
<% else %>
  <a href="<%= new_cat_url %>">Create cat!</a>
  <a href="<%= new_cat_rental_request_url %>">Rent a cat!</a>
<% end %>

